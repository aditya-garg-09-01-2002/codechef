#include <bits/stdc++.h>

using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        string s;
        cin>>s;
        int o=0,z=0;
        for(int i=0;i<n;i++)
        if(s[i]=='1')
        o++;
        else
        z++;
        if(o>=z)
        for(int i=0;i<o;i++)cout<<1;
        else
        for(int i=0;i<z;i++)cout<<0;
        cout<<endl;
    }
}
