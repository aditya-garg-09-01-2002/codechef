#include <bits/stdc++.h>

using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        vector<vector<int>>v(n);
        vector<int>vt;
        map<pair<int,int>,int>m;
        for(int i=0;i<n;i++)
        {
            int a;
            cin>>a;
            vt.push_back(a);
        }
        sort(vt.begin(),vt.end());
        for(int i=0;i<n;i++)
        {
            int a=vt[i];
            int j=0;
            while(v[j].size()>=a)
            {
                j++;
            }
            v[j].push_back(a);
        }
        int ans=0;
        for(int i=0;i<n;i++)
        {
            if(v[i].size()>0)
            ans++;
        }
        cout<<ans<<endl;
    }
}
